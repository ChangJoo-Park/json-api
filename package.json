{
  "name": "json-api",
  "version": "3.0.0-beta.10",
  "description": "A library for constructing JSON-API compliant responses",
  "homepage": "https://github.com/ethanresnick/json-api",
  "repository": {
    "type": "git",
    "url": "git://github.com/ethanresnick/json-api.git"
  },
  "author": {
    "name": "Ethan Resnick",
    "email": "ethan.resnick@gmail.com"
  },
  "license": "LGPL-3.0",
  "main": "build/src/index.js",
  "typings": "build/src/index.d.ts",
  "nyc": {
    "include": [
      "src/*.ts",
      "src/*.js",
      "src/**/*.ts",
      "src/**/*.js"
    ],
    "exclude": [
      "typings",
      "node_modules",
      "test",
      "templates",
      "build",
      "lib/index.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "@types/debug": "0.0.29",
    "@types/depd": "^1.1.32",
    "@types/express": "^4.11.0",
    "@types/lodash": "^4.14.93",
    "@types/node": "^8.5.9",
    "@types/pluralize": "0.0.27",
    "@types/pug": "^2.0.4",
    "@types/qs": "^6.5.1",
    "@types/ramda": "^0.24.18",
    "@types/url-template": "^2.0.28",
    "content-type": "^1.0.4",
    "dasherize": "2.0.x",
    "debug": "^3.1.0",
    "depd": "^1.1.2",
    "flat": "^1.2.1",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "negotiator": "github:ethanresnick/negotiator#full-parse-access",
    "pluralize": "0.0.11",
    "pug": "^2.0.0-rc.4",
    "qs": "^6.5.1",
    "ramda": "^0.24.1",
    "raw-body": "2.x.x",
    "supports-color": "^4.5.0",
    "url-template": "^2.0.4",
    "vary": "^1.1.2"
  },
  "devDependencies": {
    "@ethanresnick/tslint-config": "^5.5.1",
    "@types/chai-subset": "^1.3.1",
    "@types/mocha": "^2.2.46",
    "@types/mongodb": "^2.2.20",
    "@types/mongoose": "^4.7.32",
    "@types/superagent": "^3.5.6",
    "chai": "^4.0.0",
    "chai-subset": "^1.6.0",
    "coveralls": "^2.13.3",
    "cpr": "^2.2.0",
    "eslint": "0.x.x",
    "express": "^4.16.2",
    "gulp": "^3.8.6",
    "istanbul": "0.3.x",
    "mocha": "^2.5.3",
    "mongoose": "^4.13.9",
    "node-mongoose-fixtures": "^0.2.4",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "sinon": "1.10.2",
    "superagent": "^3.8.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "peerDependencies": {
    "mongoose": "^4.7.0",
    "express": "^4.0.0"
  },
  "scripts": {
    "clean": "rimraf build",
    "lint": "npm run lint:src & npm run lint:test",
    "lint:src": "tslint --project tsconfig.json --format verbose --exclude 'test/**/*' --config node_modules/@ethanresnick/tslint-config/tslint.json",
    "lint:test": "tslint --project tsconfig.json --format verbose --exclude 'src/**/*' --config ./test/tslint.json",
    "test": "export NODE_ENV=testing; mocha --compilers ts:ts-node/register --recursive test/unit/ test/integration/index.ts --full-trace --check-leaks",
    "test:debug": "npm run test -- --inspect-brk",
    "build:lintfree": "npm run clean && cpr templates build/templates && echo Using TypeScript && tsc --version && tsc --pretty",
    "build": "npm run build:lintfree",
    "prepare": "npm run build:lintfree",
    "prepublishOnly": "npm run test",
    "cover": "NODE_ENV=test nyc npm t",
    "cover:view": "npm run cover && open coverage/lcov-report/index.html",
    "watch": "npm run build -- --watch",
    "watch:lintfree": "npm run build:lintfree -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "bugs": {
    "url": "https://github.com/ethanresnick/json-api/issues"
  },
  "keywords": [
    "json-api",
    "jsonapi",
    "api",
    "hypermedia",
    "rest",
    "restful"
  ],
  "engines": {
    "node": ">=6.x.x"
  }
}
